<%= form_for @cohort do |f| %>
<div>
  <%= f.label :cohort_name %>
  <%= f.text_field :cohort_name %>
</div>
<div>
  <%= f.label :start_date %>
  <%= f.date_field :start_date %>
</div>
<div>
  <%= f.label :end_date %>
  <%= f.date_field :end_date %>
</div>
<div>
  <%= f.label :instructor_teaching %>
  <%= f.text_field :instructor_teaching%>
</div>
<div>
  <%= f.label :students_in_cohort %>
  <%= f.text_field :students_in_cohort %>
</div>
<div>
  <%= f.label :course_id %>
  <%= f.select :course_id, options_for_select(@courses.collect{|c| [c.subject, c.id]}, {include_blank: 'Select Course'}) %>
</div>
<div>
  <%= f.submit %>
</div>
<% end %>
